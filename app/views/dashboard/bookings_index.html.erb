<div class="container-mybookings">
  <!-- <div class="row row-mybookings"> -->


      <div class="row" style="padding: 0px 20px 0px 20px;">
        <h1 style="margin-top: 60px; padding-left: 9px;">All my booked classes:</h1>
        <% @bookings.each do |booking| %>
          <div class="col-xs-12 col-sm-6" style="margin-left: -5px; margin-right: 5px;">
            <div class="card card-spacing"
              style="background-image: linear-gradient(rgba(0,0,0,0.3),rgba(0,0,0,0.2)), url('<%= booking.course.photo.url %>')">
            <div class="card-category">Popular</div>
            <div class="card-description">
              <h2><%= link_to booking.course.name, course_path(booking.course) %></h2>
              <p>Very cool city, the best</p>
            </div>
          </div>
        </div>
      </row>
    <% end %>
  <!-- </div> -->
</div>




